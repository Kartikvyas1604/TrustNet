{
  "name": "trustnet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "next build",
    "build:backend": "cd backend && npm run build",
    "start": "next start",
    "start:backend": "cd backend && npm run start",
    "lint": "eslint",
    "setup": "npm install && cd backend && npm install",
    "db:setup": "cd backend && npm run prisma:generate && npm run prisma:push",
    "db:seed": "cd backend && npm run prisma:seed",
    "deploy:verifiers": "TS_NODE_PROJECT=tsconfig.hardhat.json npx hardhat run backend/scripts/deploy-verifiers.ts",
    "deploy:hook": "TS_NODE_PROJECT=tsconfig.hardhat.json npx hardhat run backend/scripts/deploy-privacy-hook.ts",
    "test:deploy": "./test-deployment.sh"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "circomlib": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-animate": "^0.2.10",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.32",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@uniswap/v4-core": "^1.0.2",
    "@uniswap/v4-periphery": "^1.0.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "hardhat": "^2.19.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
